package com.javatpoint;  

public class A {  
	B bObj;
	CTest c; 
	
	A() {
		System.out.println("a is created");
	}
	
	public B getB() {
		return bObj;  
	}
	
	public void setB(B bObj) {
		this.bObj = bObj;  
	}
	
	void print() {
		System.out.println("hello a");
	}
	
	void display() {
		print();
		bObj.print();
		c.print();
	} 
} 