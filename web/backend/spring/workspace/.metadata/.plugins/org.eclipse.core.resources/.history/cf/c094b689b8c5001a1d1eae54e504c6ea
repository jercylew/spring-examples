package com.javatpoint;  

public class A {  
	B bObject;
	CTest cObj; 
	
	A() {
		System.out.println("a is created");
	}
	
	public B getB() {
		return b1;  
	}
	
	public void setB1(B bObj) {
		this.b1 = bObj;  
	}
	
	public void setC(CTest cTest) {
		this.cObj = cTest;
	}
	
	public CTest getC() {
		return this.cObj;
	}
	
	void print() {
		System.out.println("hello a");
	}
	
	void display() {
		print();
		b1.print();
		cObj.print();
	} 
} 