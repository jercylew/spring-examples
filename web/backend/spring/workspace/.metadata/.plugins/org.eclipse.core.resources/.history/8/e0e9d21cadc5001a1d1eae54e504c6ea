package com.javatpoint;  

public class A {  
	B bObj;
	
	A() {
		System.out.println("a is created");
	}
	
	public B getB() {
		return bObj;  
	}
	
	public void setB(B b) {
		this.bObj = b;  
	}
	
	void print() {
		System.out.println("hello a");
	}
	
	void display() {
		print();
		bObj.print();
	} 
} 