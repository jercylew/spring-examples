package com.javatpoint;  

public class A {  
	B bObj;
	CTest cObj; 
	
	A() {
		System.out.println("a is created");
	}
	
	public B getB() {
		return bObj;  
	}
	
	public void setB(B bObj) {
		this.bObj = bObj;  
	}
	
	public void setC(CTest cTest) {
		this.cObj = cTest;
	}
	
	public CTest getC() {
		return this.cObj;
	}
	
	void print() {
		System.out.println("hello a");
	}
	
	void display() {
		print();
		bObj.print();
		cObj.print();
	} 
} 